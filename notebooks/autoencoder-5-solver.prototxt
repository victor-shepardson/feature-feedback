net: "autoencoder-5.prototxt"
test_iter: 100
test_interval: 500
test_compute_loss: true
average_loss: 500
base_lr: 0.01
lr_policy: "step"
gamma: 0.1
stepsize: 10000
display: 500
max_iter: 10000
weight_decay: 0.000
snapshot: 5000
snapshot_prefix: "autoencoder-5"
#momentum: 0.9
regularization_type: "L2"
# solver mode: CPU or GPU
solver_mode: CPU
solver_type: ADAGRAD
